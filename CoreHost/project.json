{
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.0.0-*",
      "type": "platform"
    },
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
    "YOYO.AspNetCore.Builder": "0.1.3-*",
    "YOYO.AspNetCore.ViewEngine.Razor": "0.1.3-*",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-*"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [  "dnxcore50"  ]
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "copyToOutput": [
      "Views/*.*",
      "web.config"
    ]
  },

  "runtimeOptions": {
    "gcServer": true
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "web.config"
    ]
  },

  "runtimes": {
    "win7-x64": { },
    "win7-x86": { },
    "osx.10.10-x64": { },
    "osx.10.11-x64": { },
    "ubuntu.14.04-x64": { }
  }

 
}
