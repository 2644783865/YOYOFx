{
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.0.0-*",
      "type": "platform"
    },
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
    "YOYO.AspNetCore.Builder": "0.1.3-*",
    "YOYO.AspNetCore.ViewEngine.Razor": "0.1.3-*",
    "Microsoft.NETCore.Runtime": "1.0.2-*"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
